---
import Title from '../components/typography/Title.astro';

export interface Props {
    title?: string,
}
const {title} = Astro.props;
---

<section class="py-20 bg-white dark:bg-slate-900">
    <div class="container mx-auto px-4">
        {title && <Title>{title}</Title>}
        <slot />
    </div>
</section>